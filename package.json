{
	"name": "coffee-status",
	"displayName": "Coffee Status",
	"description": "Fun coffee consumption counter",
	"version": "0.0.1",
	"publisher": "TabzyRagu",
	"icon": "https://github.com/TabzyRagu/vscodex-coffee/blob/master/coffeecup.png",
	"repository": "https://github.com/TabzyRagu/vscodex-coffee",
	"issues": "https://github.com/TabzyRagu/vscodex-coffee/issues",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "coffee.getCoffeeCount",
				"title": "Get Coffee Count"
			},
			{
				"command": "coffee.addCoffeeCount",
				"title": "Add Coffee to Count"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
